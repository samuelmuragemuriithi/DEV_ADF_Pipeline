{
	"name": "Netflix_Pipeline",
	"properties": {
		"activities": [
			{
				"name": "If if_Schedule_Name_Not_Set",
				"type": "IfCondition",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@not(empty(pipeline().parameters.schedule_name))",
						"type": "Expression"
					},
					"ifFalseActivities": [
						{
							"name": "Fail_No shedule_name",
							"type": "Fail",
							"dependsOn": [],
